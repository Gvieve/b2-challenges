<%= form_with url: "/professors/#{@professor.id}", method: :patch, local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name, value: @professor.name %><br><br>
<%= f.label :age %>
<%= f.number_field :age, value: @professor.age %><br><br>
<%= f.label :specialty %>
<%= f.text_field :specialty,value:  @professor.specialty %><br><br>
<%= f.submit "Update" %>
<% end %>
